{
  "name": "indigo-react",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "build": "vite build",
    "server": "json-server --port 3001 --watch db.json --host 127.0.0.1",
    "start": "concurrently --kill-others \"npm run dev\" \"npm run server\""
  },
  "dependencies": {
    "axios": "^1.7.7",
    "concurrently": "^9.0.1",
    "framer-motion": "^11.3.30",
    "json-server": "^1.0.0-beta.3",
    "react": "^18.3.1",
    "react-animate-on-scroll": "^2.1.9",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.8.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "gh-pages": "^6.1.1",
    "globals": "^15.9.0",
    "vite": "^5.4.0"
  }
}
